{
  "*": { "make": "mocha" },
  "commands/*.js": {
    "type": "command",
    "alternate": "spec/commands/{}_spec.js"
  },
  "spec/*.js": {
    "type": "spec",
    "alternate": "{}.js",
    "template": [
      "var assert = require('assert');",
      "",
      "describe({}Command, function() {",
      "})",
      ""
    ]
  }
}
